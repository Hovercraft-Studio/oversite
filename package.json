{
  "name": "oversite",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "vite": "vite --host --port 3002",
    "dev": "concurrently \"npm run vite\" \"npm run server\"",
    "prebuild": "npm install @rollup/rollup-linux-x64-gnu --no-save || true",
    "server": "nodemon --watch ./src/server --ext mjs node ./server.mjs --portHttp 3003",
    "build": "vite build",
    "build_skip": "echo 'Skipping build on Heroku'",
    "vite-preview": "vite preview",
    "preview": "npm run build && npm run server",
    "start": "node ./server.mjs",
    "ws": "node ./server/ws-relay.mjs --port 3003",
    "server-debug": "nodemon --watch ./src/server --ext mjs node ./server.mjs --port 3003 --portHttp 3003 --debug",
    "node-app-store-demo": "nodemon --watch ./examples/app-store/nodejs/node-app-demo --ext mjs node ./examples/app-store/nodejs/node-app-demo/index.mjs --server ws://127.0.0.1:3003/ws --portHttp 3003",
    "node-dashboard-demo": "nodemon --watch ./examples/dashboard/js-backend --ext mjs node ./examples/dashboard/js-backend/index.mjs --server ws://127.0.0.1:3003/ws",
    "launch-chrome": "node ./scripts/run-chrome.mjs --url http://localhost:3002 --location 500,100 --fullscreen true --screen-num 1"
  },
  "dependencies": {
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "http-proxy": "^1.18.1",
    "nodemon": "^3.1.9",
    "notyf": "^3.10.0",
    "screenshot-desktop": "^1.15.1",
    "vite": "^6.3.2",
    "websocket": "^1.0.34",
    "ws": "^8.18.1"
  }
}
